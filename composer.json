    "version": "0.5.0",
    "description": "A cms-kit wizard to edit different markups",
    "license": "GPL",
    "authors": [
        {
            "name": "Christoph Taubmann",
            "email": "info@cms-kit.com"
        }
    ],
    "homepage": "http://cms-kit.com",

    "require": {
        "php": ">=5.3.3",
        "cmskit/package-installer": "1.0.*@dev",
        "cmskit/jquery-ui": "dev-master",
        "cmskit/ace-editor": "dev-master",
        "cmskit/wizard-simple-filemanager": "dev-master"
